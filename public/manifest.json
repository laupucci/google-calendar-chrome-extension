
{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Google Calendar Extension",
  "description": "This is a Chrome extension built with React and TypeScript for google calendar",
  "action": {
      "default_popup": "index.html",
      "default_title": "Google Calendar Event Creator"
  },
  "permissions": ["storage", "identity", "identity.email"],
  "host_permissions": [
    "https://*.googleapis.com/*",
    "https://web.whatsapp.com/*",
    "https://apis.google.com/"
  ],
  "oauth2": {
    "client_id": "$VITE_CLIENT_ID",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "icons": {
   "16": "calendar16.png",
   "32": "calendar32.png",
   "48": "calendar48.png",
   "128": "calendar128.png"
 }
}
